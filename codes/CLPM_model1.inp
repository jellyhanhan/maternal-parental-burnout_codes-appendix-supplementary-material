TITLE:CLPM_basic model1 自回归和相关

  DATA:file is cross legged pannel model.dat;
    variable:
    MISSING ARE ALL (999);

   names are in1 in2 ex1 ex2
  bur1 sds1 ses bur2 sds2 age;

    USEVARIABLES = sds1 in1 in2 ex1 ex2 ses bur1 bur2 sds2 age;

    Analysis:
    type is GENERAL;
    estimator is MLR;
      !BOOTSTRAP=5000;

    model:
  !自回归
  sds2 on sds1(a1);bur2 on bur1(a2);
  ex2 on ex1(a3);in2 on in1(a4);

 
  ex2 in2 on ses;
  ex2 in2 on age;

  sds1 with bur1;
  bur1 with in1;
  in1 with ex1;
  sds1 with in1;
  bur1 with ex1;
  sds1 with ex1;

  sds2 with bur2(e1);
  bur2 with in2(e2);
  in2 with ex2(e3);
  sds2 with in2(e4);
  bur2 with ex2(e5);
  sds2 with ex2(e6);



  output:
   standardized modindices(all);
   TECH1 STDYX sampstat cinterval;
            !STAND CINT(BCBOOTSTRAP);